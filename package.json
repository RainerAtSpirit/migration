{
  "name": "coras-boilerplate",
  "version": "1.0.0",
  "description": "Coras Boilerplate",
  "main": "index.js",
  "repository": "https://github.com/CorasWorks/Boilerplate",
  "author": "Coras frontend team",
  "license": "UNLICENSED",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "precommit": "lint-staged",
    "clean": "shx rm -rf _bundles lib",
    "test": "jest -u",
    "prod": "npm run clean && npm run webpack -- --env.mode production",
    "build": "npm run prod",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "prod:debug": "npm run debug -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode development"
  },
  "dependencies": {
    "@coras/corejs": "^2.8.0",
    "@types/lodash-es": "^4.17.1",
    "csstips": "^0.3.0",
    "csx": "^9.0.0",
    "is-hotkey": "^0.1.3",
    "lodash-es": "^4.17.11",
    "mobx": "4",
    "mobx-react": "^5.2.8",
    "mobx-router5": "^4.2.3",
    "mobx-state-tree": "^3.3.0",
    "moment": "^2.22.2",
    "node-uuid": "^1.4.8",
    "react": "^16.5.0",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^16.5.0",
    "react-loadable": "^5.5.0",
    "react-mobx-router5": "^6.1.1",
    "router5": "^6.5.0",
    "semantic-ui-react": "^0.82.5",
    "typestyle": "^2.0.1",
    "workbox-webpack-plugin": "^3.5.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.5.2",
    "critters-webpack-plugin": "^1.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "moment-locales-webpack-plugin": "^1.0.7",
    "prerender-loader": "^1.1.0",
    "prettier": "^1.14.2",
    "shx": "^0.3.2",
    "style-loader": "^0.23.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
